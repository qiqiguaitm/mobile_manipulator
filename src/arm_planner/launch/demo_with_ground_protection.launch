<launch>
  <!-- Demo with ground collision protection -->
  <!-- 使用主demo.launch，它会自动加载已配置地面保护的with_gripper版本 -->
  
  <include file="$(find arm_planner)/launch/demo.launch">
    <arg name="gripper" value="true" />
    <arg name="mode" default="real" />
    <arg name="use_rviz" default="true" />
  </include>
  
</launch>