<launch>
  <arg
    name="model" />
    
  <!-- Load the URDF directly since we need the v100 camera version -->
  <param
    name="robot_description"
    command="$(find xacro)/xacro '$(find mobile_manipulator2_description)/urdf/mobile_manipulator2_with_v100_camera.xacro'" />
    
  <node
    name="joint_state_publisher_gui"
    pkg="joint_state_publisher_gui"
    type="joint_state_publisher_gui" />
  <node
    name="robot_state_publisher"
    pkg="robot_state_publisher"
    type="robot_state_publisher" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find mobile_manipulator2_description)/rviz/display.rviz" />
</launch>