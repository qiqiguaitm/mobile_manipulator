<launch>
  <!-- Launch the rslidar driver with pointcloud to laserscan conversion -->
  <include file="$(find lidar_driver)/launch/rslidar_driver.launch"/>
  
  <!-- Launch the lidar driver node -->
  <node pkg="lidar_driver" type="lidar_driver_node.py" name="lidar_driver_node" output="screen"/>
  
  <!-- Launch the test listener node -->
  <node pkg="lidar_driver" type="lidar_test.py" name="lidar_test_listener" output="screen"/>
</launch>