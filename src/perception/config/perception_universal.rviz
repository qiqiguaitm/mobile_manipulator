Panels:
  - Class: rviz/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /TF1
      Splitter Ratio: 0.6
    Tree Height: 700
  - Class: rviz/Time
    Name: Time
    SyncMode: 0
    SyncSource: ""
Visualization Manager:
  Class: ""
  Displays:
    # ===== 基础显示 =====
    - Alpha: 0.5
      Cell Size: 1
      Class: rviz/Grid
      Color: 160; 160; 164
      Enabled: true
      Line Style:
        Line Width: 0.03
        Value: Lines
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 10
      Reference Frame: <Fixed Frame>
      Value: true
    
    # TF显示
    - Class: rviz/TF
      Enabled: true
      Frame Timeout: 15
      Frames:
        All Enabled: false
        base_link:
          Value: true
        mobile_base:
          Value: true
        arm_base:
          Value: true
        camera1_link:
          Value: false
        camera2_link:
          Value: false
        camera3_link:
          Value: false
      Marker Scale: 0.3
      Name: TF
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Tree:
        {}
      Update Interval: 0
      Value: true
    
    # 机器人模型
    - Alpha: 1
      Class: rviz/RobotModel
      Collision Enabled: false
      Enabled: true
      Links:
        All Links Enabled: true
        Expand Joint Details: false
        Expand Link Details: false
        Expand Tree: false
        Link Tree Style: Links in Alphabetic Order
      Name: Robot Model
      Robot Description: robot_description
      TF Prefix: ""
      Update Interval: 0
      Value: true
      Visual Enabled: true
    
    # ===== 手部相机显示组 =====
    - Class: rviz/Group
      Displays:
        # 手部抓取2D可视化
        - Class: rviz/Image
          Enabled: true
          Image Topic: /perception/hand/vis_image
          Max Value: 1
          Median window: 5
          Min Value: 0
          Name: Hand Grasp 2D
          Normalize Range: true
          Queue Size: 1
          Transport Hint: compressed
          Unreliable: false
          Value: true
        
        # 手部抓取3D标记
        - Class: rviz/MarkerArray
          Enabled: true
          Marker Topic: /perception/hand/grasp_markers
          Name: Hand Grasp 3D Markers
          Namespaces:
            {}
          Queue Size: 100
          Value: true
        
        # 手部抓取点云
        - Alpha: 1
          Autocompute Intensity Bounds: true
          Autocompute Value Bounds:
            Max Value: 10
            Min Value: -10
            Value: true
          Axis: Z
          Channel Name: intensity
          Class: rviz/PointCloud2
          Color: 0; 255; 0
          Color Transformer: FlatColor
          Decay Time: 0
          Enabled: true
          Invert Rainbow: false
          Max Color: 255; 255; 255
          Min Color: 0; 0; 0
          Name: Hand Grasp Points
          Position Transformer: XYZ
          Queue Size: 10
          Selectable: true
          Size (Pixels): 5
          Size (m): 0.01
          Style: Points
          Topic: /perception/hand/grasp_points
          Unreliable: false
          Use Fixed Frame: true
          Use rainbow: true
          Value: true
        
        # 手部Referring 2D可视化
        - Class: rviz/Image
          Enabled: true
          Image Topic: /perception/hand/referring_vis
          Max Value: 1
          Median window: 5
          Min Value: 0
          Name: Hand Referring 2D
          Normalize Range: true
          Queue Size: 1
          Transport Hint: raw
          Unreliable: false
          Value: true
        
        # 手部Referring 3D标记
        - Class: rviz/MarkerArray
          Enabled: true
          Marker Topic: /perception/hand/referring_markers
          Name: Hand Referring 3D Markers
          Namespaces:
            {}
          Queue Size: 100
          Value: true
        
        # 手部Referring点云
        - Alpha: 1
          Autocompute Intensity Bounds: true
          Autocompute Value Bounds:
            Max Value: 10
            Min Value: -10
            Value: true
          Axis: Z
          Channel Name: intensity
          Class: rviz/PointCloud2
          Color: 255; 255; 0
          Color Transformer: FlatColor
          Decay Time: 0
          Enabled: true
          Invert Rainbow: false
          Max Color: 255; 255; 255
          Min Color: 0; 0; 0
          Name: Hand Referring Points
          Position Transformer: XYZ
          Queue Size: 10
          Selectable: true
          Size (Pixels): 5
          Size (m): 0.01
          Style: Points
          Topic: /perception/hand/referring_points
          Unreliable: false
          Use Fixed Frame: true
          Use rainbow: true
          Value: true
      Enabled: true
      Name: Hand Camera
    
